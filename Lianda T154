int main()
{
    int sub1, sub2, sub3, sub4, sub5;

    sub1 = getnum1 ();
    sub2 = getnum2 ();
    sub3 = getnum3 ();
    sub4 = getnum4 ();
    sub5 = getnum5 ();

    addnumber (sub1, sub2, sub3, sub4, sub5);

}
    void addnumber (int a, int b, int c, int d, int e)
    {

    int result;
    result=a+b+c+d+e;
    result=(a+b+c+d+e)/5;
    printf("Your final grade: %d",result);
}
